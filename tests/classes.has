# base class
class Dog {
	func new(name) {
		# Create attribute inside of class
		this.Name = name;
	}

	func showName() {
		println(this.Name);
	}

	func bark() {
		println("Woof!");
	}
}

# Inherits all the methods from the base class.
class BigDog : Dog {
	func growl() {
		println("GRRRR");
	}
	# Overloads the bark() method from the base class.
	func bark() {
		println("Big dog bark");
	}
}

func main() {
	fiddo = new Dog ("fiddo");
	fiddo.bark ();

	ralph = new BigDog ("Ralph");
	ralph.showName ();

	ralph.bark ();
}
