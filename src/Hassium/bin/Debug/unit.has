func main () {
	a = 4;
	b = 5;
	if (a > 4 || a < 4 || a != 4)
		return 1;
	println("Passed negative if");
	if (b >= 5 && b <= 5 && b == 5) {
		println("Passed positive if");
	} else {
		return 1;
	}
	c = [ 1, 2, 3, 4, 5 ];
	if (c.length != 5)
		return 1;
	println("Passed list length!");
